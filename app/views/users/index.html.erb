<h1><strong>Users listing page</strong></h1>
<table>
<thead>
<tr>
<th>id</th>
<th>username</th>
<th>email</th>
<th>Show</th>
<th>Edit</th>
<th>Delete</th>
</thead>
</tr>
<tbody>
<% @users.each do|user| %>
    <tr>
    <td><%= user.id %></td>
    <td><%= user.username %></td>
    <td><%= user.email %></td>
    <td><%= link_to 'show', user_path(user.id) %></td>
    <td><%= link_to 'edit', edit_user_path(user.id) %></td>
    <td><%= link_to 'delete', user_path(user.id), method: :delete %></td>
    </tr>
    <% end %>
    </tbody>
</table>

